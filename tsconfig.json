{
  "compilerOptions": {
    "baseUrl": ".",
      "paths": {
        "@database": ["data-base/db.ts"]
      }
    ,
    "target": "es6",
    "allowJs": false,
    "lib": ["dom", "dom.iterable", "esnext"],
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve"
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx"
, "hook/useAddCar.ts"  ],
  "exclude": [
    "node_modules"
  ]
}
